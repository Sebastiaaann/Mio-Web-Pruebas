	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "1. Login HOMA (Intercambio Token)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"if (jsonData.success) {",
									"    var token = jsonData.token;",
									"    var patient_id = jsonData.patient_id;",
									"    console.log(\"!!! TOKEN RECEIVED !!!\", token);",
									"    ",
									"    // 1. Postman Standard Scopes",
									"    if (pm.collectionVariables) { pm.collectionVariables.set(\"token\", token); pm.collectionVariables.set(\"patient_id\", patient_id); }",
									"    if (pm.environment) { pm.environment.set(\"token\", token); pm.environment.set(\"patient_id\", patient_id); }",
									"    if (pm.globals) { pm.globals.set(\"token\", token); pm.globals.set(\"patient_id\", patient_id); }",
									"",
									"    // 2. Bruno Specific (Force Env & Var)",
									"    if (typeof bru !== 'undefined') {",
									"        console.log(\"!!! BRUNO DETECTED !!!\");",
									"        if (bru.setVar) { bru.setVar(\"token\", token); bru.setVar(\"patient_id\", patient_id); }",
									"        if (bru.setEnvVar) { bru.setEnvVar(\"token\", token); bru.setEnvVar(\"patient_id\", patient_id); }",
									"    }",
									"    console.log(\"Token updated in all available scopes\");",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"{{email}}\",\n    \"UID\": \"{{firebase_uid}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/authorizations",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"authorizations"
							]
						},
						"description": "Intercambia el UID de Firebase y Email por un token de sesión HOMA."
					}
				}
			]
		},
